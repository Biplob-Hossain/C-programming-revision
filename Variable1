#include<stdio.h>

int main(){


    //there is a two system to declare variable 1. this is one type
    int ID, ID1; // First Declared variable. rules: 1st variable Type, 2nd: variable name

    ID=10, ID1=20; //second: initialized value of variable. means put the value

    printf(" ID is the %d\n",ID);
    printf("ID1 is %d\n",ID1);

    return 0;




}
